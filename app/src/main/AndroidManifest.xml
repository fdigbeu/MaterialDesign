<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lveapp.fr.materialdesign">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".View.Activities.MainActivity"
            android:screenOrientation="sensorPortrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".View.Activities.LoginActivity"
            android:label="@string/formLoginPassword"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".View.Activities.BottomSheetsActivity"
            android:label="@string/bottomSheets"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".View.Activities.BottomNavActivity"
            android:label="@string/bottomNavigation"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".View.Activities.NavDrawerActivity"
            android:label="@string/title_activity_nav_drawer"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>